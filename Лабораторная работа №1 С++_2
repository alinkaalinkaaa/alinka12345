#include <iostream>
#include <vector>
#include <cctype>  
using namespace std;

int main() {
    vector<char> original = { 'a', '1', 'b', '2', 'c', '3' };
    vector<char> letters; 
    vector<char> numbers; 

    for (char c : original) { 
        if (isalpha(c)) {
            letters.push_back(c); 
        else if (isdigit(c)) {  
            numbers.push_back(c); 
        }
    }

    original.clear();

    for (char c : letters) { 
        cout << c;
    }
    cout << endl;

    for (char c : numbers) {
        cout << c;
    }
    cout << endl;

    return 0;
}
